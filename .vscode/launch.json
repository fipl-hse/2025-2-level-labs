    "version": "0.2.0",
    "configurations": [
        {
            "name": "[GITHUB] Update forks",
            "type": "debugpy",
            "request": "launch",
            "program": "config/github/update_forks.py",
            "args": [
                "--config", "config/github/update_fork_config.json"
            ],
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        }
    ]
}
